<div id="fm-fixed-mobile" class="mob-fix-panel topmm-fix d-none">
	<div class="mobile-sidebar-search hidden shtop">
		<div class="mobile-sidebar-search__top">
			<div class="mobile-sidebar-search__title">{{ text_search }}</div>
			<span class="mobile-sidebar-search__close fm-close-search"><i class="up-icon-close" aria-hidden="true"></i></span>
		</div>
		<div class="mobile-sidebar-search__content"></div>
	</div>
	<div class="mobile-sidebar-viewed hidden">
		<div class="mobile-sidebar-viewed__top no-shadow">
			<div class="mobile-sidebar-viewed__title">{{ text_ai_viewed }}</div>
			<span class="mobile-sidebar-viewed__close fm-close-viewed"><i class="up-icon-close" aria-hidden="true"></i></span>
		</div>
		<div class="mobile-sidebar-viewed__content"></div>
	</div>
	<div class="mobile-sidebar-phones hidden">
		<div class="mobile-sidebar-phones__top no-shadow">
			<div class="mobile-sidebar-phones__title">{{ text_ai_contact }}</div>
			<span class="mobile-sidebar-phones__close fm-close-phones"><i class="up-icon-close" aria-hidden="true"></i></span>
		</div>
		<div class="mobile-sidebar-phones__content"><div class="mobile-sidebar-phones__inner"></div></div>
	</div>
	<div class="mob-menu-info-fixed-left hidden">
		<div class="mob-first-menu active" id="mobm-left-content">
			<div class="mobm-top">
				<div class="mobm-title">{{ text_menu }}</div>
				<div class="mobile-first-tel">
					{% if header_phones[0].type %}
						<a {{ header_phones[0].type }}>{{ header_phones[0].phone }}</a>
					{% else %}
						<span>{{ header_phones[0].phone }}</span>
					{% endif %}
					<div class="up-header-phones__text-af">{{ text_after_phone }}</div>
				</div>
				<button type="button" class="mobm-close-menu" aria-label="Close" onclick="close_mob_menu();">
					<i class="up-icon-close" aria-hidden="true"></i>
				</button>
			</div>
			<div class="mobm-body">
				{% if (language is not empty or currency is not empty) or (theme_mode_status is defined and theme_mode_status == 1) %}
				<div class="mobm-header-block">
					{% if theme_mode_status is defined and theme_mode_status == 1 %}
						<div class="mh-left-b mob-theme-mode">
							<span class="mob-text-theme-mode">{{ text_theme_mode }}</span>
							<div class="up-theme-mode">
								<button class="up-theme-btn light-theme-btn{% if theme_class == 'light-theme' %} active{% endif %}" onclick="setTheme('light-theme')">
									<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="5"/><path d="M12 1v2M12 21v2M4.2 4.2l1.4 1.4M18.4 18.4l1.4 1.4M1 12h2M21 12h2M4.2 19.8l1.4-1.4M18.4 5.6l1.4-1.4"/></svg>
								</button>
								<button class="up-theme-btn dark-theme-btn{% if theme_class == 'dark-theme' %} active{% endif %}" onclick="setTheme('dark-theme')">
									<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M21 12.79A9 9 0 1 1 11.21 3 7 7 0 0 0 21 12.79z"></path></svg>
								</button>
							</div>
						</div>
					{% endif %}
					<div class="mh-left-b mob-language dflex align-items-center"></div>
					<div class="mh-right-b mob-currency dflex align-items-center ml-auto"></div>
				</div>
				{% endif %}
				<div class="mob-menu" id="mob-catalog-left">
					<div class="mobm-body mm-ss"></div>
				</div>
				{% if header_nav_menu_links %}
					<div class="links-mob"></div>
				{% endif %}
				{% if desc_info_mob %}
					<div class="desc_info_mob">{{ desc_info_mob }}</div>
				{% endif %}
			</div>
		</div>
	</div>
</div>
<div id="fm-fixed-mobile-bottom" class="mob-fix-panel fm_type_design_bottom_{{ fm_type_design_bottom }} hidden-md hidden-lg">
	<div class="container">
		<div class="box-flex-fix {% if show_text_after_icon and show_text_after_icon == 1 %} show-title{% endif %}">
			{% if fm_show_btn_go_home and fm_show_btn_go_home == 1 %}
				{% if home_page != 'active' %}
					<div class="mob-m-i mob-m-i__home">
						<a class="btn-fm-bottom btn btn-go-home dflex align-items-center justify-content-center active-bg" href="{{ home }}">
							<i class="up-icon-22 up-icon-home" aria-hidden="true"></i>
							{% if show_text_after_icon and show_text_after_icon == 1 %}
								<span class="text-a-icon">{{ text_ai_home }}</span>
							{% endif %}
						</a>
					</div>
				{% endif %}
			{% endif %}
			{% if fb_widget.status and fb_widget.status == 1 %}
				<div class="mob-m-i box-feedback-widget">
					<button class="btn-fm-bottom btn-open-chats dflex align-items-center justify-content-center" aria-label="{{ text_ai_viewed }}" type="button">
						<svg class="icon-chats" xmlns="http://www.w3.org/2000/svg" width="21" height="21" fill="none" viewBox="0 0 21 21">
							<path fill="currentColor" d="M.5 10.734c0 1.12.319 2.175.957 3.165.638.989 1.49 1.796 2.559 2.421-.052.183-.111.417-.176.703-.065.287-.156.736-.274 1.348-.117.612-.162 1.12-.136 1.524.026.403.156.605.39.605.782 0 1.57-.208 2.364-.625.794-.417 1.425-.833 1.894-1.25.469-.417.716-.625.742-.625a11.06 11.06 0 0 0 4.18-.82c-.156-.052-.45-.209-.879-.47-.43-.26-.697-.39-.8-.39h-2.5c-.47 0-.964.26-1.485.782-.73.65-1.42 1.12-2.07 1.406L5.5 15.5l-.664-.586c-1.77-1.146-2.656-2.539-2.656-4.18 0-.416.078-.885.234-1.406-.104-.156-.176-.397-.215-.723a7.446 7.446 0 0 1-.058-.878v-.899l.039-.508C1.06 7.518.5 8.99.5 10.734ZM12.18.5c-2.318 0-4.29.677-5.918 2.031C4.634 3.885 3.82 5.513 3.82 7.414c0 1.875.808 3.412 2.422 4.61 1.615 1.197 3.594 1.796 5.938 1.796.026 0 .293.28.8.84.508.56 1.179 1.12 2.012 1.68.833.56 1.64.84 2.422.84.182 0 .293-.143.332-.43.04-.287.02-.631-.058-1.035a24.29 24.29 0 0 0-.274-1.23 52.729 52.729 0 0 0-.273-1.055l-.157-.43c1.068-.651 1.92-1.465 2.559-2.441.638-.977.957-2.025.957-3.145 0-1.901-.814-3.529-2.441-4.883C16.43 1.177 14.47.5 12.179.5Zm3.984 11.094-1.094.664.43 1.25a5.8 5.8 0 0 1 .195.664c.079.312.156.56.235.742-.573-.39-1.25-.976-2.031-1.758-.6-.65-1.146-.976-1.641-.976-1.823 0-3.392-.423-4.707-1.27-1.315-.846-1.973-2.012-1.973-3.496-.026-1.458.606-2.695 1.895-3.71C8.762 2.686 10.33 2.18 12.18 2.18c1.849 0 3.418.507 4.707 1.523 1.289 1.016 1.933 2.253 1.933 3.711 0 1.64-.885 3.034-2.656 4.18Z"/>
						</svg>
						<svg class="icon-close-chats" xmlns="http://www.w3.org/2000/svg" width="14" height="14" fill="none" viewBox="0 0 14 14">
							<path fill="currentColor" d="m8.542 7 4.703-4.692A1.098 1.098 0 1 0 11.692.755L7 5.458 2.307.755A1.098 1.098 0 1 0 .754 2.308L5.457 7 .754 11.692a1.094 1.094 0 0 0 0 1.553 1.094 1.094 0 0 0 1.553 0L7 8.542l4.692 4.703a1.093 1.093 0 0 0 1.553 0 1.094 1.094 0 0 0 0-1.553L8.542 7Z"/>
						</svg>
						{% if show_text_after_icon and show_text_after_icon == 1 %}
							<span class="text-a-icon">{{ text_ai_chats }}</span>
						{% endif %}
					</button>
					<div class="mobile-widget-block d-none">
						{% for result in fb_data %}
							<a class="mobile-widget-block__item" {{ result.fbw_type }}>
								<span class="mobile-widget-block__icon">
									<img loading="lazy" width="38" height="38" alt="{{ result.fbw_icon }}" src="catalog/view/theme/upstore/stylesheet/feedback_widget/icon/{{ result.fbw_icon }}.svg">
								</span>
								<span class="mobile-widget-block__text">{% if fb_widget.display_type == 1 %}{{ result.fbw_text }}{% endif %}</span>
							</a>
						{% endfor %}
					</div>
				</div>
			{% endif %}
			<div class="mob-m-i fm-viewed-bottom">
				<button class="btn-fm-bottom btn-open-viewed dflex align-items-center justify-content-center" {% if show_text_after_icon == 0 or fm_type_design_bottom == 3 %}aria-label="{{ text_ai_viewed }}"{% endif %} type="button">
					<i class="up-icon-22 up-icon-quickview" aria-hidden="true"></i>
					{% if quantity_viewed > 0 %}
						<span class="viewed-quantity">{{ quantity_viewed }}</span>
					{% endif %}
					{% if show_text_after_icon and show_text_after_icon == 1 %}
						<span class="text-a-icon">{{ text_ai_viewed }}</span>
					{% endif %}
				</button>
			</div>
			<div class="mob-m-i box-wishlist-mob">
				<a rel="nofollow" {% if show_text_after_icon == 0 or fm_type_design_bottom == 3 %}aria-label="{{ text_ai_wishlist }}"{% endif %} class="btn-fm-bottom btn-mob-wishlist-bottom dflex align-items-center justify-content-center" href="{{ wishlist }}">
					<i class="up-icon-22 up-icon-wishlist" aria-hidden="true"></i>
					{% if total_wishlist > 0 %}{{ text_wishlist }}{% endif %}
					{% if show_text_after_icon and show_text_after_icon == 1 %}
						<span class="text-a-icon">{{ text_ai_wishlist }}</span>
					{% endif %}
				</a>
			</div>
			<div class="mob-m-i box-compare-mob">
				<a rel="nofollow" {% if show_text_after_icon == 0 or fm_type_design_bottom == 3 %}aria-label="{{ text_ai_compare }}"{% endif %} class="btn-fm-bottom btn-mob-compare-bottom dflex align-items-center justify-content-center" href="{{ compare }}">
					<i class="up-icon-22 up-icon-compare" aria-hidden="true"></i>
					{% if total_compare > 0 %}{{ text_compare }}{% endif %}
					{% if show_text_after_icon and show_text_after_icon == 1 %}
						<span class="text-a-icon">{{ text_ai_compare }}</span>
					{% endif %}
				</a>
			</div>
			{% if not fb_widget.status or fb_widget.status == 0 %}
				{% if fm_type_design_bottom != 3 %}
					<div class="mob-m-i menu_fix_mob">
						<button {% if show_text_after_icon == 0 or fm_type_design_bottom == 3 %}aria-label="{{ text_ai_menu }}"{% endif %} type="button" class="btn-fm-bottom btn btn-menu-mobile dflex align-items-center justify-content-center" onclick="open_mob_menu_left()">
							<i class="up-icon-22 up-icon-menu-grid" aria-hidden="true"></i>
							{% if show_text_after_icon and show_text_after_icon == 1 %}
								<span class="text-a-icon">{{ text_catalog }}</span>
							{% endif %}
						</button>
					</div>
				{% endif %}
			{% endif %}
		</div>
	</div>
</div>
