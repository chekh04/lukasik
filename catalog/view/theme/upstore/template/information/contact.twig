{{ header }}
<div class="container">
	<ul class="breadcrumb">
		{% for i,breadcrumb in breadcrumbs %}
		{% if (i+1 < breadcrumbs|length) %}<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li> {% else %}<li>{{ breadcrumb['text'] }}</li>{% endif %}
	{% endfor %}
	</ul>
	<div class="row">{{ column_left }}
		{% if (column_left and column_right) %}
		{% set class = 'col-sm-4 col-md-6' %}
		{% elseif (column_left or column_right) %}
		{% set class = 'col-sm-8 col-md-9' %}
		{% else %}
		{% set class = 'col-sm-12' %}
		{% endif %}
		<div id="content" class="{{ class }}">
		{{ content_top }}
			<h1>{{ heading_title }}</h1>

			{% if (ch_contact_map_code is defined) %}
				<div class="row ch_contact_map_code">
					<div class="col-sm-12">{{ ch_contact_map_code }}</div>
				</div>
			{% endif %}
			<div class="row-flex pb-20">
				<div class="col-xs-12 col-sm-6 col-md-6">
				<div class="box-contact-left h-100">
					<div class="contact-title">{{ store }}</div>
					{% if (ch_contact_address is defined) %}
					<div class="contact_address">
						{{ ch_contact_address }}
					</div>
					{% endif %}

					{% if (ch_contact_image is not empty) %}
					<div class="contact_image">
						<img loading="lazy" src="{{ ch_contact_image }}" alt="{{ store }}">
					</div>
					{% endif %}


					{% if ch_contacts_phone or up_viber or up_whatsapp or up_telegram or up_messenger %}
						<div class="contact-title">{{ text_contact_info }}</div>
						<ul class="list-unstyled ch_contacts_phone">
							{% if up_viber %}
								<li>
									<a class="d-inline-flex align-items-center" href="viber://chat?number={{ up_viber }}" target="_blank">
										<span class="up-contact-icon mr-10">
											<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
