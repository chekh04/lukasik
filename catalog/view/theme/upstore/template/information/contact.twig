							{% endif %}
							{% if up_messenger %}
								<li>
									<a class="d-inline-flex align-items-center" href="https://m.me/{{ up_messenger }}" target="_blank">
										<span class="up-contact-icon mr-10">
											<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
												<path fill="#3F70FE" d="M12 1C6.113 1 1.28 5.59 1.28 11.274c0 3.029 1.402 5.72 3.573 7.594V23l3.965-1.983c1.008.304 2.062.531 3.182.531 5.888 0 10.72-4.59 10.72-10.274C22.72 5.59 17.889 1 12 1Zm0 1.787c4.966 0 8.934 3.811 8.934 8.487S16.966 19.761 12 19.761a9.356 9.356 0 0 1-3.043-.53l-.363-.112-1.954.977v-2.01l-.335-.28c-1.983-1.562-3.239-3.904-3.239-6.532 0-4.676 3.968-8.487 8.934-8.487Zm-1.005 5.668L5.607 14.15l4.83-2.68 2.568 2.764 5.333-5.78-4.719 2.653-2.624-2.652Z"/>
											</svg>
										</span>
										Messenger
									</a>
								</li>
							{% endif %}
							{% if (ch_contacts_phone is not empty) %}
								{% for result_p in ch_contacts_phone %}
									<li class="item-phone"><a href="tel:{{ result_p['tel'] }}" target="_blank">{{ result_p['text'] }}</a></li>
								{% endfor %}
							{% endif %}
						</ul>
					{% endif %}

					{% if (ch_contact_email is defined) %}
					<div class="contact_email">
						{{ ch_contact_email }}
					</div>
					{% endif %}
					{% if (ch_contact_working_time is defined) %}
					<div class="contact-title">{{ text_open }}</div>
						<ul class="list-unstyled">
							{{ ch_contact_working_time }}
						</ul>
					{% endif %}
				</div>
				</div>
				<div class="col-xs-12 col-sm-6 col-md-6 box-contact-form">
					<form action="{{ action }}" method="post" enctype="multipart/form-data" class="form-horizontal h-100">
						<fieldset>
							<legend>{{ text_contact }}</legend>
							<div class="form-group required">
								<div class="col-sm-12">
									<label class="control-label" for="input-name">{{ entry_name }}</label>
									<input type="text" name="name" value="{{ name }}" id="input-name" class="form-control" />
									{% if (error_name) %}
									<div class="text-danger">{{ error_name }}</div>
									{% endif %}
								</div>
							</div>
							<div class="form-group required">
								<div class="col-sm-12">
									<label class="control-label" for="input-email">{{ entry_email }}</label>
									<input type="text" name="email" value="{{ email }}" id="input-email" class="form-control" />
									{% if (error_email) %}
									<div class="text-danger">{{ error_email }}</div>
									{% endif %}
								</div>
							</div>
							<div class="form-group required">
								<div class="col-sm-12">
									<label class="control-label" for="input-enquiry">{{ entry_enquiry }}</label>
									<textarea name="enquiry" rows="5" id="input-enquiry" class="form-control">{{ enquiry }}</textarea>
									{% if (error_enquiry) %}
									<div class="text-danger">{{ error_enquiry }}</div>
									{% endif %}
								</div>
							</div>
							{{ captcha }}
						</fieldset>
						<div class="buttons">
							<div class="pull-left">
								<input class="btn btn-primary" type="submit" value="{{ button_submit }}" />
							</div>
						</div>
					</form>
				</div>
			</div>

			{% if store_locations %}
				<div class="row store-locations">
					{% for store_location in store_locations %}
						<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
							<div class="store-locations__item">
								<div class="store-locations__image">
									<div class="swiper swiper-banner swiper-location">
										<div class="swiper-wrapper">
											<div class="item swiper-slide">
												<img src="{{ store_location.image }}" alt="{{ store_location.title }}" title="{{ store_location.title }}" class="img-responsive br-10" />
											</div>
											{% if store_location.images is not empty %}
												{% for image in store_location.images %}
													<div class="item swiper-slide">
														<img decoding="async" loading="lazy" src="{{ image.thumb }}" alt="{{ store_location.title }}" title="{{ store_location.title }}" class="img-responsive" />
													</div>
												{% endfor %}
											{% endif %}
										</div>
										{% if store_location.images is not empty %}
											<div class="swiper-banner-pagination swiper-pagination"></div>
										{% endif %}
									</div>
								</div>
								<div class="store-locations__info">
									<div class="store-locations__info-store-name">{{ store_location.title }}</div>
									<div class="store-locations__info-address">
										<div>{{ store_location.address }}</div>
										{% if store_location.geocode %}
											<a href="https://www.google.com/maps/dir/Current+Location/{{ store_location.geocode }}" target="_blank" class="btn btn-primary mt-10">{{ text_get_directions }}</a>
										{% endif %}
									</div>

									{% if store_location.sl_phones or store_location.viber or store_location.whatsapp or store_location.telegram or store_location.messenger %}
										<div class="store-locations__info-contacts">
											<div class="store-locations__info-title">{{ text_contact_info }}</div>
											<ul class="list-unstyled">
												{% for sl_phone in store_location.sl_phones %}
													<li><a href="tel:{{ sl_phone.tel }}" target="_blank">{{ sl_phone.text }}</a></li>
												{% endfor %}
											</ul>
											{% if store_location.email %}
												<div class="contact_email mb-10">{{ store_location.email }}</div>
											{% endif %}
											{% if store_location.viber or store_location.whatsapp or store_location.telegram or store_location.messenger %}
												<div class="chat-links d-flex">
													{% if store_location.viber %}
														<a class="d-inline-flex align-items-center" href="viber://chat?number={{ store_location.viber }}" target="_blank">
