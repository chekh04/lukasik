<script>

var total_execution_time = 0;
var startTime = new Date().getTime();
var buttonOriginalText = '';

function setSeoData(button) {
	var $button = $(button);
	var seoType = $button.data('seo');
	var progressBarId = $button.data('progress');
	var methodType = $button.data('method');

	total_execution_time = 0;
	startTime = new Date().getTime();

	updateSeoData($button.attr('id'), progressBarId, methodType, seoType);
}

function updateSeoData(buttonId, progressBarId, methodType, seoType, start = 0, totals = 0) {
	var limit = 100;
	var $button = $('#' + buttonId);

	if (start === 0) {
		buttonOriginalText = $button.html();
	}

	$.ajax({
		type: 'get',
		url: 'index.php?route=extension/module/upstore_seo_tool/' + methodType + '&user_token={{ user_token }}&start=' + start + '&limit=' + limit + '&totals=' + totals + '&seo_type=' + seoType,
		dataType: 'json',
		beforeSend: function() {
			$('.alert, .text-danger').remove();
			$button.html('<i class="fa fa-spinner fa-spin"></i> ' + buttonOriginalText).prop('disabled', true);
		},
		success: function(json) {
			$('.alert, .text-danger').remove();
			if (json['error']) {
				$button.before('<div class="alert alert-danger">' + json['error'] + '</div>');
			}

			if (!json['error'] && !$(progressBarId).length) {
				$button.before('<div class="progress"><div id="' + progressBarId.replace('#', '') + '" class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"><span class="sr-only">0% Complete</span></div></div>');
			}

			var progress = json['progress'];
			var start = json['start'];
			var totals = json['totals'];
			var currentTime = new Date().getTime();
			var execution_time = (currentTime - startTime) / 1000;
			total_execution_time += execution_time;

			$(progressBarId).css('width', progress + '%').attr('aria-valuenow', progress).find('.sr-only').text(progress + '% Complete');

			if (start < totals) {
				startTime = new Date().getTime();
				updateSeoData(buttonId, progressBarId, methodType, seoType, start, totals);
			} else {
				console.log('Time: ' + total_execution_time.toFixed(2) + ' sec');
				setTimeout(function () {
					$button.html(buttonOriginalText).prop('disabled', false);
				}, 300);

				if(!json['error']){
					$button.before('<div class="alert alert-success">' + json['success'] + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
				}
			}
		}
	});
}

$(document).ready(function () {
	let cursorPosition = false;
	let lastFocusedInput = null;

	$(document).on('focus', '.cursor-position', function () {
		lastFocusedInput = $(this);
		cursorPosition = getCaretPosition(this);
	});

	$(document).on('keyup click', '.cursor-position', function () {
		cursorPosition = getCaretPosition(this);
	});

	$(document).on('click', '.seo-variable__item', function () {
		const variable = $(this).data('code');
		const targetSelector = $(this).closest('.seo-variables-container').data('target');
		const inputField = $(targetSelector);

		if (!inputField.length) return;

		let value = inputField.val();
		let newValue;

		if (cursorPosition === false) {
			newValue = value.trim() + ' ' + variable;
		} else {
			const beforeCursor = value.slice(0, cursorPosition).trimEnd();
			const afterCursor = value.slice(cursorPosition).trimStart();
			newValue = `${beforeCursor} ${variable} ${afterCursor}`;
			cursorPosition = beforeCursor.length + variable.length + 2;
		}

		inputField.val(newValue.trim());
		setTimeout(() => {
			setCaretPosition(inputField[0], cursorPosition);
			inputField.focus();
		}, 0);
	});

	$(document).on('click', '.seo-url-variable__item', function () {
		const variable = $(this).data('code');
		const targetSelector = $(this).closest('.seo-variables-container').data('target');
		const inputField = $(targetSelector);

		if (!inputField.length) return;

		let value = inputField.val();
		let newValue;

		if (cursorPosition === false) {
			newValue = value.trim() + variable;
		} else {
			const beforeCursor = value.slice(0, cursorPosition).trimEnd();
			const afterCursor = value.slice(cursorPosition).trimStart();
			newValue = `${beforeCursor}${variable}${afterCursor}`;
			cursorPosition = beforeCursor.length + variable.length + 1;
		}

		inputField.val(newValue.trim());
		setTimeout(() => {
			setCaretPosition(inputField[0], cursorPosition);
			inputField.focus();
		}, 0);
	});

	function getCaretPosition(input) {
		if (!input) return false;
		return input.selectionStart || 0;
	}

	function setCaretPosition(input, pos) {
		if (!input.setSelectionRange) return;
		input.setSelectionRange(pos, pos);
	}
});

