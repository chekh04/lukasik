					<label class="col-sm-2 control-label">{{ entry_lang_prefix }}</label>
					<div class="col-sm-4 col-md-2 col-auto">
						{% for language in languages %}
							<div class="input-group">
								<span class="input-group-addon"><img src="language/{{ language['code'] }}/{{ language['code'] }}.png" title="{{ language['name'] }}" /></span>
								<input name="upstore_seo_tool_setting[lang_prefix][{{ language['language_id'] }}]" value="{{ upstore_seo_tool_setting['lang_prefix'][language['language_id']] ?? '' }}" class="form-control"/>
							</div>
						{% endfor %}
					</div>
				</div>
				<ul class="nav nav-tabs">
					<li class="active"><a href="#tab-seo-products" data-toggle="tab">{{ tab_seo_products }}</a></li>
					<li><a href="#tab-seo-category" data-toggle="tab">{{ tab_seo_category }}</a></li>
					<li><a href="#tab-seo-brand" data-toggle="tab">{{ tab_seo_brand }}</a></li>
					<li><a href="#tab-seo-information" data-toggle="tab">{{ tab_seo_information }}</a></li>
					<li><a href="#tab-seo-news" data-toggle="tab">{{ tab_seo_news }}</a></li>
				</ul>
